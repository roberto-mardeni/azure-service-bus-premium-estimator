{
    "version": "2.0",
    "functionTimeout": "00:10:00",
    "healthMonitor": {
        "enabled": true,
        "healthCheckInterval": "00:00:10",
        "healthCheckWindow": "00:02:00",
        "healthCheckThreshold": 6,
        "counterThreshold": 0.80
    },
    "logging": {
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    },
    "extensions": {
        "serviceBus": {
            "clientRetryOptions":{
                "tryTimeout": "00:10:00",
                "maxRetries": 5
            },
            "autoCompleteMessages": true
        }
    }
}