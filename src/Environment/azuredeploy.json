{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "prefix": {
            "type": "string",
            "defaultValue": "servicebusestimator"
        },
        "queueName": {
            "type": "string",
            "defaultValue": "myqueue"
        },
        "storageName": {
            "type": "string",
            "defaultValue": "[concat('estimator', uniqueString(resourceGroup().id))]"
        }
    },
    "variables": {
        "estimatorPlanName": "[concat(parameters('prefix'),'-plan')]",
        "estimatorAppName": "[concat(parameters('prefix'),'-app')]",
        "receiver1PlanName": "[concat(parameters('prefix'),'receiver1-plan')]",
        "receiver1AppName": "[concat(parameters('prefix'),'receiver1-app')]",
        "receiver2PlanName": "[concat(parameters('prefix'),'receiver2-plan')]",
        "receiver2AppName": "[concat(parameters('prefix'),'receiver2-app')]",
        "receiver3PlanName": "[concat(parameters('prefix'),'receiver3-plan')]",
        "receiver3AppName": "[concat(parameters('prefix'),'receiver3-app')]"
    },
    "resources": [
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "shared",
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "relativePath": "nested/azuredeploy-shared.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "prefix": {
                        "value": "[parameters('prefix')]"
                    },
                    "queueName": {
                        "value": "[parameters('queueName')]"
                    },
                    "storageName": {
                        "value": "[parameters('storageName')]"
                    },
                    "appName": {
                        "value": "[variables('estimatorPlanName')]"
                    },
                    "receiverName": {
                        "value": "[variables('receiverAppName')]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "estimator",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'shared')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "relativePath": "nested/azuredeploy-estimator.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "planName": {
                        "value": "[variables('estimatorPlanName')]"
                    },
                    "appName": {
                        "value": "[variables('estimatorAppName')]"
                    },
                    "storageConnectionString": {
                        "value": "[reference('shared').outputs.storageConnectionString.value]"
                    },
                    "serviceBusConnectionString": {
                        "value": "[reference('shared').outputs.serviceBusConnectionString.value]"
                    },
                    "appInsightsInstrumentationKey": {
                        "value": "[reference('shared').outputs.appInsightsInstrumentationKey.value]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "receiver1",
            "resourceGroup": "[concat(parameters('prefix'),'Receiver')]",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'shared')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "relativePath": "nested/azuredeploy-receiver.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "planName": {
                        "value": "[variables('receiver1PlanName')]"
                    },
                    "appName": {
                        "value": "[variables('receiver1AppName')]"
                    },
                    "storageConnectionString": {
                        "value": "[reference('shared').outputs.storageConnectionString.value]"
                    },
                    "serviceBusConnectionString": {
                        "value": "[reference('shared').outputs.serviceBusConnectionString.value]"
                    },
                    "appInsightsInstrumentationKey": {
                        "value": "[reference('shared').outputs.appInsightsInstrumentationKey.value]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "receiver2",
            "resourceGroup": "[concat(parameters('prefix'),'Receiver')]",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'shared')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "relativePath": "nested/azuredeploy-receiver.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "planName": {
                        "value": "[variables('receiver2PlanName')]"
                    },
                    "appName": {
                        "value": "[variables('receiver2AppName')]"
                    },
                    "storageConnectionString": {
                        "value": "[reference('shared').outputs.storageConnectionString.value]"
                    },
                    "serviceBusConnectionString": {
                        "value": "[reference('shared').outputs.serviceBusConnectionString.value]"
                    },
                    "appInsightsInstrumentationKey": {
                        "value": "[reference('shared').outputs.appInsightsInstrumentationKey.value]"
                    }
                }
            }
        },
        {
            "type": "Microsoft.Resources/deployments",
            "apiVersion": "2021-04-01",
            "name": "receiver3",
            "resourceGroup": "[concat(parameters('prefix'),'Receiver')]",
            "dependsOn": [
                "[resourceId('Microsoft.Resources/deployments', 'shared')]"
            ],
            "properties": {
                "mode": "Incremental",
                "templateLink": {
                    "relativePath": "nested/azuredeploy-receiver.json",
                    "contentVersion": "1.0.0.0"
                },
                "parameters": {
                    "planName": {
                        "value": "[variables('receiver3PlanName')]"
                    },
                    "appName": {
                        "value": "[variables('receiver3AppName')]"
                    },
                    "storageConnectionString": {
                        "value": "[reference('shared').outputs.storageConnectionString.value]"
                    },
                    "serviceBusConnectionString": {
                        "value": "[reference('shared').outputs.serviceBusConnectionString.value]"
                    },
                    "appInsightsInstrumentationKey": {
                        "value": "[reference('shared').outputs.appInsightsInstrumentationKey.value]"
                    }
                }
            }
        }
    ],
    "outputs": {}
}